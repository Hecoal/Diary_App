@model List<DiaryEntry>
<div class="container">
    <div class="row pt-4">
        <div class="col-6">
            <a class="btn btn-primary">
                Create New Diary Entrie
            </a>           
        </div>
        <div class="col-6 text-end">
            <h2>Diary Entries</h2>
        </div>
    </div>
</div>
<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Title</th>
                <th>Content</th>
                <th>Date</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var entry in Model){
                <tr>
                    <td>@entry.Title</td>
                    <td>@entry.Content</td>
                    <td>@entry.Created</td>
                    <td>Edit and delete</td>
                </tr>
            }
        </tbody>
    </table>
</div>
